<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>ApiSdk</PackageId>
    <Version>1.0.0</Version>
    <Authors>API SDK Team</Authors>
    <Company>API SDK</Company>
    <Description>Cross-platform SDK that abstracts calls to both flat file storage (series of JSON files) and an OTA API</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/your-org/api-sdk</RepositoryUrl>
    <PackageTags>api;sdk;ota;json</PackageTags>
  </PropertyGroup>

</Project>
